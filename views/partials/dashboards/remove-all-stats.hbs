<main
  id="remove-allstats"
  class="remove-page js-dashboard dashboard clear-header flx flx-col"
  data-page-label="User Dashboard"
>
  <div class="mw-860 flx flx-col remove-allstats-container">
    <section class="remove-dashboard-section">
      <h2 class="remove-allstats-title">Removal Stats</h2>
      <ul class="remove-allstats-list">
        <li class="remove-allstats-item">
          <span class="bold">Total Results:</span>
          {{stats.totalResults}}
        </li>
        <li class="remove-allstats-item">
          <span class="bold">Removed Results:</span>
          {{stats.removedResults}}
        </li>
        <li class="remove-allstats-item">
          <span class="bold">Removal Success Rate:</span>
          {{stats.resolutionPct}}%
        </li>
        <li class="remove-allstats-item flx flx-col align-start">
          <span class="bold">Resolution Time (Days):</span>
          <ul class="remove-allstats-time-list">
            <li class="remove-allstats-time-item">
              <span class="bold">Mean:</span>
              {{stats.resolutionTimeData.mean}}
            </li>
            <li class="remove-allstats-time-item">
              <span class="bold">Mode:</span>
              {{stats.resolutionTimeData.mode}}
            </li>
            <li class="remove-allstats-time-item">
              <span class="bold">Average:</span>
              {{stats.resolutionTimeData.avg}}
            </li>
            <li class="remove-allstats-time-item">
              <span class="bold">Std Dev:</span>
              {{stats.resolutionTimeData.stdDev}}
            </li>
          </ul>
        </li>
      </ul>
    </section>
  </div>
</main>